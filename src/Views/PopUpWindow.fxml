<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pane" prefHeight="140.0" prefWidth="260.0" style="-fx-background-color: #1f2e3d;" styleClass="pane" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.PopUpWindowController">
    <Label fx:id="labelMessage" alignment="CENTER" layoutX="2.0" layoutY="25.0" prefHeight="30.0" prefWidth="260.0" styleClass="pane" text="Label" textFill="WHITE">
        <font>
            <Font name="System Bold" size="20.0" />
        </font>
    </Label>
    <Button fx:id="closeButton" layoutX="116.0" layoutY="81.0" mnemonicParsing="false" onAction="#closeWindow" text="OK">
      <font>
         <Font name="System Bold" size="12.0" />
      </font></Button>
</AnchorPane>
